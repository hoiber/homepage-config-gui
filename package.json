{
  "name": "homepage-config-gui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.536.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "server": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm start\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "docker:build": "docker build -t homepage-config-gui .",
    "docker:run": "docker run -p 8080:80 homepage-config-gui",
    "docker:run-dev": "docker run -p 8080:80 --name homepage-config-gui-dev homepage-config-gui",
    "docker:run-live": "docker run -p 8080:3001 -v /path/to/homepage/config:/config -e ENABLE_LIVE_UPDATES=true homepage-config-gui",
    "docker:stop": "docker stop homepage-config-gui-dev",
    "docker:clean": "docker rm homepage-config-gui-dev"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17"
  }
}
